{
  "name": "druglaneserver",
  "version": "2.0.0",
  "description": "Druglane Pharmacy Management Software Version 2",
  "main": "dist/main.js",
  "scripts": {
    "pack": "electron-builder --dir",
    "dist": "webpack && electron-builder",
    "test": "cross-env NODE_ENV=test jest --testTimeout=100000",
    "db:create:test": "cross-env NODE_ENV=test npx sequelize-cli db:create",
    "db:reset": "node npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "build": "npm run clean && tsc && npm run copy-files",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles -u 1 src/**/*.html src/**/*.css dist/",
    "watch": "npm run clean && tsc --watch && npm run copy-files",
    "start": "npm run watch && electron ./dist/main.js",
    "dev": "concurrently \"npm run clean && npm run copy-files && tsc --watch \" \"nodemon --exec  electron dist/main  \" ",
    "migration-skeleton": "npx sequelize-cli migration:generate --name migration-skeleton",
    "start-webpack": "npm run build-webpack && electron dist/main.js",
    "build-webpack": "webpack",
    "react:dev": "webpack serve --mode=development",
    "electron:dev": "nodemon",
    "webpack-dev": "concurrently --kill-others \"npm run react:dev\" \"npm run electron:dev\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carlwuaku/druglaneposserver.git"
  },
  "author": "Calron Softwares Ltd.",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/carlwuaku/druglaneposserver/issues"
  },
  "homepage": "https://github.com/carlwuaku/druglaneposserver#readme",
  "dependencies": {
    "@types/jquery": "^3.5.14",
    "axios": "^1.3.4",
    "bcryptjs": "^2.0.1",
    "body-parser": "^1.18.2",
    "decompress-zip": "^0.2.2",
    "dotenv": "^10.0.0",
    "ejs": "^3.1.3",
    "electron-context-menu": "^3.6.1",
    "electron-store": "^8.1.0",
    "express": "^4.16.3",
    "express-fileupload": "^0.4.0",
    "express-session": "^1.17.1",
    "express-winston": "^4.0.4",
    "firebase-admin": "^9.7.0",
    "image-to-base64": "^2.2.0",
    "ip": "^1.1.5",
    "jquery": "^3.6.1",
    "mariadb": "^3.1.1",
    "md5": "^2.2.1",
    "mongoose": "^6.8.0",
    "node-fetch": "^2.6.1",
    "node-os-utils": "^1.2.2",
    "node-schedule": "^1.3.2",
