    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "sequelize": "^6.3.4",
    "sequelize-typescript": "^2.1.3",
    "socket.io": "^2.1.1",
    "sqlite-async": "^1.1.0",
    "sqlite3": "^5.0.0",
    "umzug": "^3.2.1",
    "winston": "^3.0.0",
    "xlsx": "^0.16.5"
